{
  "expo": {
    "name": "ColorCatalog",
    "description": "Catalog your colors. Save your favorite!",
    "slug": "ColorCatalog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cc-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.webharan.ColorCatalog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
